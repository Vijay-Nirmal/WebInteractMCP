{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.SignalR": "Information",
      "WebInteractMCPServer": "Information"
    }
  },
  "AllowedHosts": "*",
  "McpInteract": {
    "Client": {
    },
    "Tool": {
      "TimeoutMinutes": 5,
      "EnableDetailedErrorLogging": false
    },
    "Cors": {
      "AllowAnyOrigin": true
    }
  }
}
